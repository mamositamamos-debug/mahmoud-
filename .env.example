# HealthHub+ Environment Variables

# Node environment
NODE_ENV=production

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=username
DB_PASSWORD=yourpassword
DB_NAME=healthhub

# API configuration
API_URL=https://api.healthhub.com

# JWT configuration
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=1h

# Redis configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Logging configuration
LOG_LEVEL=info